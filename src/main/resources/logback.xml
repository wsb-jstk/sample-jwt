<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="ConsoleEncoder" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd} | %d{HH:mm:ss.SSS} | %-10.-10thread | %-5p | %-85(%class{0}#%method\(%3line\)) | %message%n</pattern>
			<!--<pattern>%d{yyyy-MM-dd} | %d{HH:mm:ss.SSS} | %-10.-10thread | %-5p | %message%n</pattern>-->
			<charset>utf8</charset>
		</encoder>
	</appender>
	<root level="debug">
		<appender-ref ref="ConsoleEncoder"/>
	</root>
</configuration>
